{
  "name": "vaultprojectui",
  "version": "1.0.0",
  "description": "HasiCorp Vault Web UI",
  "main": "index.js",
  "scripts": {
    "start": "node src/start-ssr.native",
    "test": "VAULT_TOKEN= b3bcd3ef-7753-348d-7e8c-aca56794acbf VAULT_ADDR='http://127.0.0.1:8200' mocha --compilers js:babel-register --require ./tests/helpers --reporter spec **/*.test.js",
    "build": "run-s clean build:static build:ssr",
    "clean": "rm -rf dist",
    "fix": "eslint --fix --ext .jsx src/index.jsx",
    "f": "eslint --fix",
    "dev:static": "webpack-dev-server --config webpack.config.static.js --colors --progress",
    "build:static": "webpack --config \"./webpack.config.static.js\" --colors --display-error-details",
    "dev:ssr": "run-p watch:webpack:ssr nodemon:ssr",
    "build:ssr": "webpack --config \"./webpack.config.ssr.js\" --colors --display-error-details",
    "watch:webpack:ssr": "webpack --watch --config webpack.config.ssr.js --colors --display-error-details",
    "nodemon:ssr": "LOG_LEVEL=debug PORT=8000 nodemon src/start-ssr.native --watch dist/ssr/index.js",
    "cors": "curl --header \"X-Vault-Token:  b3bcd3ef-7753-348d-7e8c-aca56794acbf\" --request PUT  --data @payload.json  http://127.0.0.01:8200/v1/sys/config/cors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s4ysolutions/vaultprojectui.git"
  },
  "author": "Sergey Dolin <sergey@s4y.solutions>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/s4ysolutions/vaultprojectui/issues"
  },
  "homepage": "https://github.com/s4ysolutions/vaultprojectui#readme",
